<?xml version="1.0" encoding="UTF-8"?>
<ui version="4.0">
    <class>MyApp</class>

    <widget class="QMainWindow" name="MyApp">
        <!-- Menu -->
        <widget class="QMenuBar" name="menuBar">

            <widget class="QMenu" name="fileMenu">
                <property name="title">
                    <string>file</string>
                </property>
                <addaction name="actionOpenProfile"/>
                <addaction name="actionSaveProfile"/>
                <addaction name="actionRemoveProfile"/>
            </widget>
            <addaction name="fileMenu"/>
            <addaction name="actionSetFolderProfile"/>
            <addaction name="actionStartGame"/>
        </widget>

        <!-- Menu actions -->
        <action name="actionOpenProfile">
            <property name="text">
                <string>open profile</string>
            </property>
        </action>

        <action name="actionSetFolderProfile">
            <property name="text">
                <string>set folder profile</string>
            </property>
        </action>

        <action name="actionSaveProfile">
            <property name="text">
                <string>save profile</string>
            </property>
        </action>

        <action name="actionRemoveProfile">
            <property name="text">
                <string>remove profile</string>
            </property>
        </action>

        <action name="actionStartGame">
            <property name="text">
                <string>start game</string>
            </property>
        </action>

        <!-- Contenido contundente -->
        <widget class="QWidget" name="centralWidget">
            <layout class="QVBoxLayout" name="mainLayout">
                <!-- Perfil seleccionado, y perfil de folder -->
                <item>
                    <layout class="QHBoxLayout" name="hboxProfile">

                        <item>
                            <widget class="QLabel" name="labelProfile">
                                <property name="text">
                                    <string>profile: </string>
                                </property>
                            </widget>
                        </item>
                        <item>
                            <widget class="QLabel" name="labelCurrentProfile">
                                <property name="text">
                                    <string>current profile</string>
                                </property>
                            </widget>
                        </item>

                        <item>
                            <widget class="QLabel" name="labelFolderProfile">
                                <property name="text">
                                    <string>folder profile: </string>
                                </property>
                            </widget>
                        </item>
                        <item>
                            <widget class="QLabel" name="labelCurrentFolderProfile">
                                <property name="text">
                                    <string>current folder profile</string>
                                </property>
                            </widget>
                        </item>

                    </layout>
                </item>

                <!-- Seccion de widgets -->
                <item>
                    <widget class="QTabWidget" name="tabWidget">
                        <!-- Tabs QWidget, no se agragan aca. Pero se puede.-->
                    </widget>
                </item>

            </layout>
        </widget>

    </widget>

    <resources/>
    <connections/>
</ui>
